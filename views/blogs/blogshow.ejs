<% layout('/layouts/boilerplate') -%>
<body>
    <div class="latestContainer">
        <div class="blog-details">
          <h1><%= blog.title %></h1>
          <p><%= blog.author %></p>
          <img src="<%= blog.image %>" alt="Error loading image" class="blog-image">
          <p class="blogContent"><%= blog.content %></p>
        </div>
    </div>
</body>