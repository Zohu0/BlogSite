<% layout('/layouts/boilerplate') -%>

    <body>
        <% for(let blog of allBlogs){ %>
            <div>
                <p>
                    <a href="/blogs/<%= blog._id %>"><b>
                            <%= blog.title %>
                        </b></a>
                </p>
            </div>
            <% } %>


                <% for (let blog=allBlogs.length - 5; blog < allBlogs.length; blog++) { %>
                    <div>
                        <p>
                            <a href="/blogs/<%= blog._id %>">
                                <b><%= allBlogs[blog].title %></b>
                            </a>
                        </p>
                    </div>
                    <% } %>


    </body>